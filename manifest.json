{
    "manifest_version": 2,
    "name": "utahrealestateWatcher",
    "description": "Watch as new real estate populates on utahrealestate.com",
    "version": "0.1.0",
    "permissions": [
        "storage",
        "<all_urls>"
    ],
    "icons": {
        "32": "images/icon.png",
        "128": "images/logo.png"
    },
    "background": {
        "scripts": [
            "lib/lodash.js",
            "lib/jquery.js",
            "src/background/background.js"
        ]
    },
    "browser_action": {
        "default_title": "Listings",
        "default_popup": "src/popup/main.html"
    }
}
